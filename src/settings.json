{
    "sharedNamespace": "zz-shared",
    "sharedConfigmap": "jobman-jobs-config",
    "job": {
        "defaultImage": "default",
        "gpuResName": "nvidia.com/gpu",
        "userConfigmap": null,
        "datasetsEnvVar": "CHAIMELEON_DATASETS",
        "mountPoints": {
            "datalake": "/mnt/datalake",
            "persistent_home": "/home/chaimeleon/persistent-home",
            "persistent_shared_folder": "/home/chaimeleon/persistent-shared-folder",
            "datasets": "/home/chaimeleon/datasets"
        },
        "affinity": {
            "cpu": "NODE_WITH_CPUS_NO_GPU",
            "gpu": "NODE WITH GPU"
        },
        "requests": {
            "cpu": 1,
            "memory": 1000
        },
        "limits": {
            "cpu": 1,
            "memory": 1000
        }
    },
    "kubeConfig": {
        "type": "file",
        "file": "/kube_config.json"
    },
    "harbor": {
        "url": "https://harbor.chaimeleon-eu.i3m.upv.es",
        "project": "chaimeleon-library-batch"
    }
}